<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.topico1.bancoedua.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_create"
        android:name="com.topico1.bancoedua.ui.create.CreateFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_create" />

    <fragment
        android:id="@+id/navigation_transactions"
        android:name="com.topico1.bancoedua.ui.transactions.TransactionsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_transactions" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.topico1.bancoedua.ui.profile.ProfileFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_transactions" />
</navigation>